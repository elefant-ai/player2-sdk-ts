{
  "name": "@player2-game/sdk",
  "version": "0.1.0",
  "description": "TypeScript client generated from Player2 Web API OpenAPI spec",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json && node ./scripts/build-cjs.mjs"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.49.0",
    "typescript": "^5.6.2",
    "rimraf": "^6.0.1"
  }
}
